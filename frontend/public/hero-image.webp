<svg xmlns="http://www.w3.org/2000/svg" width="500" height="300" viewBox="0 0 500 300">
  <rect width="500" height="300" fill="#f3f4f6"/>
  
  <!-- Laptop -->
  <rect x="150" y="100" width="200" height="130" rx="5" fill="#333"/>
  <rect x="160" y="110" width="180" height="110" fill="#0077cc"/>
  <rect x="190" y="230" width="120" height="10" fill="#333"/>
  <rect x="240" y="240" width="20" height="5" fill="#333"/>
  
  <!-- Tools -->
  <circle cx="400" cy="80" r="20" fill="#ddd"/>
  <rect x="395" y="70" width="10" height="20" fill="#333"/>
  
  <rect x="120" y="180" width="30" height="5" fill="#ddd" transform="rotate(45, 135, 180)"/>
  <circle cx="120" cy="180" r="10" fill="#ddd"/>
  
  <!-- Circuit Lines -->
  <line x1="50" y1="50" x2="100" y2="50" stroke="#0077cc" stroke-width="2"/>
  <line x1="50" y1="50" x2="50" y2="100" stroke="#0077cc" stroke-width="2"/>
  <circle cx="50" cy="50" r="5" fill="#0077cc"/>
  
  <line x1="400" y1="200" x2="450" y2="200" stroke="#0077cc" stroke-width="2"/>
  <line x1="450" y1="200" x2="450" y2="250" stroke="#0077cc" stroke-width="2"/>
  <circle cx="450" cy="250" r="5" fill="#0077cc"/>
  
  <!-- Technician Silhouette -->
  <path d="M80,220 Q90,210 100,220 L100,270 L80,270 Z" fill="#333"/>
  <circle cx="90" cy="200" r="15" fill="#333"/>
</svg> 